<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m.table="urn:import:gwt.material.design.client.ui.table">

	<g:HTMLPanel>
		<m.table:MaterialDataTable ui:field="table"
			shadow="1" height="calc(100vh - 300px)" useLoadOverlay="false"
			useStickyHeader="false" useCategories="false" selectionType="SINGLE" />
	</g:HTMLPanel>
</ui:UiBinder> 